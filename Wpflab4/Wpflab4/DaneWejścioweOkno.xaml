<Window x:Class="Wpflab4.DaneWejścioweOkno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpflab4"
        mc:Ignorable="d"
        Title="DaneWejścioweOkno" Height="450" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label Content="Wysokość" HorizontalAlignment="Left" Margin="90,117,0,0" VerticalAlignment="Top" Height="44" Width="123"/>
        <Label Content="Szerokość&#xD;&#xA;" HorizontalAlignment="Left" Margin="90,183,0,0" VerticalAlignment="Top" Height="49" Width="91"/>
        <Button x:Name="btnOKWysokośćISzerokość" Content="OK" HorizontalAlignment="Left" Margin="530,165,0,0" VerticalAlignment="Top" Height="67" Width="120" Click="btnOKWysokośćISzerokość_Click" />
        <TextBox x:Name="txtWysokość" HorizontalAlignment="Left" Margin="235,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="0" x:FieldModifier="private"/>
        <TextBox x:Name="txtSzerokość" HorizontalAlignment="Left" Margin="235,198,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" x:FieldModifier="private"/>

    </Grid>
</Window>
